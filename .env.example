# OpenRouter API Key
# Get your API key from: https://openrouter.ai/keys
OPENROUTER_API_KEY=your_openrouter_api_key_here

# ElevenLabs API Key
# Get your API key from: https://elevenlabs.io/app/settings/api-keys
ELEVENLABS_API_KEY=your_elevenlabs_api_key_here

# Gemini API Key (Primary LLM Provider)
# Get your API key from: https://aistudio.google.com/app/apikey
GEMINI_API_KEY=your_gemini_api_key_here

# Cloudflare Workers AI (Secondary LLM Provider)
# Get your API key from: https://dash.cloudflare.com/profile/api-tokens
# Get your Account ID from: https://dash.cloudflare.com/ (in the URL or sidebar)
CLOUDFLARE_API_KEY=your_cloudflare_api_key_here
CLOUDFLARE_ACCOUNT_ID=your_cloudflare_account_id_here

# Honeycomb Observability (Optional but Recommended)
# Get your API key from: https://ui.honeycomb.io/account
# Sign up for free at: https://www.honeycomb.io/
HONEYCOMB_API_KEY=your_honeycomb_api_key_here
# Optional: Set to EU endpoint if using EU instance
# OTEL_EXPORTER_OTLP_ENDPOINT=https://api.eu1.honeycomb.io:443

# Python logging auto-instrumentation (optional, defaults to true)
OTEL_PYTHON_LOGGING_AUTO_INSTRUMENTATION_ENABLED=true
